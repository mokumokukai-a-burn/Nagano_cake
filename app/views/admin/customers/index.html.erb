<h2>会員一覧</h2>
<table>
  <thead>
    <tr>
      <th>会員ID</th>
      <th>氏名</th>
      <th>メールアドレス</th>
      <th>ステータス</th>
    </tr>
  </thead>
  <tbody>
    <% @customers.each do |customer| %>
      <tr>
        <td><%= customer.id %></td>
        <td><%= customer.last_name %></td>
        <td><%= customer.email %></td>
        <!--if is_deleted==false then 入会 else 退会 -->
        <td><%= customer.is_deleted %></td>
      </tr>
    <% end %>
  </tbody>
</table>

